# The rest of my fun kubernetes aliases
alias k8='kubectl'
alias k8p="kubectl proxy"
alias k8c='kubectl config'
alias k8cv='kubectl config view'
alias k8cu='kubectl config use-context'
alias k8cs='kubectl config set-context'
alias k8e='kubectl exec -ti'
alias k8a='kubectl apply -f'
alias k8log='kubectl logs -f'
alias k8pod='kubectl get pods'
